                        .media-left
                          img.img-rounded.media-object(src="assets/images/face/2.jpg", alt="" width="50")
                        .media-body
                          b Kaia O'Hara
                          p.small Greenmoon Corporation Ltd.
                    td
                      .btn-group(role="group")
                        button.btn.btn-link.text-dark(type='button'): i.fa-lg.fa.ion-ios-play
                        button.btn.btn-link.text-dark(type='button'): i.fa-lg.fa.ion-ios-pause
                        button.btn.btn-link.text-dark(type='button'): i.fa-lg.fa.ion-ios-fastforward
                    td Pencil
                    td 95
                    td: a.btn.btn-primary(href="#") View more
        = '\n'

        .panel.bg-dark
          .panel-body
            a.btn.btn-danger.pull-xs-right(href="#") Download
            ul.nav.nav-inline
              li.nav-item
                a.nav-link.text-white(href="#") Documentation
              li.nav-item
                a.nav-link.text-white(href="#") Help
              li.nav-item
                a.nav-link.text-white(href="#") Bug Report
              li.nav-item
                a.nav-link.text-white(href="#") Testimonial
        = '\n'

    = '\n'
    // #SIDEBAR
    .layout-sidebar
      = '\n'
      // #WEATHER
      // Need a padding div with aqua background
      .panel.bg-aqua-lighten(data-mh='g1')
        .panel-body
          p.pull-xs-right.small
            | Preciptation: 0%
