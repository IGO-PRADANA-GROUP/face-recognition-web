        = '\n'
        // TOP WRAPPER
        include ./layout/_topbar-wrapper.pug
        //END TOP WRAPPER
        = '\n'

        = '\n'
        // PAGE CONTENT HERE
        block main
        // END PAGE CONTENT
        = '\n'


      // END VIEW WAPPER
      = '\n'

    // END MAIN WRAPPER
    = '\n'

    = '\n'
    // WEB PERLOAD
    include ./layout/_loading.pug
    = '\n'

