  body(class="orchid "+ body_class)

    = '\n'
    // #SIDEMENU
    if menu
      include ./layout/_mainmenu-block.pug
    else
      include ./layout/_mainmenu-2-block.pug

    = '\n'
    // #MAIN
    .main-wrapper

      = '\n'
      // VIEW WRAPPER
